{
    "name": "vscode-unpkg",
    "displayName": "unpkg",
    "description": "Search and embed libraries from unpkg.com in Visual Studio Code",
    "version": "0.0.1",
    "publisher": "vladkon",
    "author": "vladkon",
    "license": "MIT",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "unpkg",
        "unpkgjs",
        "content delivery network"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Jakobud/vscode-cdnjs.git"
    },
    "bugs": {
        "url": "https://github.com/Jakobud/vscode-cdnjs/issues"
    },
    "homepage": "https://github.com/Jakobud/vscode-cdnjs",
    "activationEvents": [
        "onCommand:unpkg.search",
        "onCommand:unpkg.recentLibraries",
        "onCommand:unpkg.clearCache"
    ],
    "icon": "images/icon.png",
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "unpkg.search",
                "title": "Search for libraries",
                "category": "unpkg"
            },
            {
                "command": "unpkg.recentLibraries",
                "title": "Recent libraries",
                "category": "unpkg"
            },
            {
                "command": "unpkg.clearCache",
                "title": "Clear cache",
                "category": "unpkg"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "unpkg",
            "properties": {
                "unpkg.quoteStyle": {
                    "type": "string",
                    "default": "single",
                    "enum": [
                        "single",
                        "double"
                    ],
                    "description": "Quote style for HTML tags. Possible values are 'single' or 'double'"
                },
                "unpkg.protocol": {
                    "type": "string",
                    "default": "https://",
                    "enum": [
                        "https://",
                        "http://",
                        "//"
                    ],
                    "description": "Protocol for urls in HTML tags. Possible values are 'https://', 'http://' or '//'"
                },
                "unpkg.maxRecentLibraries": {
                    "type": "integer",
                    "default": 10,
                    "description": "The maxmium number of Recent Libraries to save"
                },
                "unpkg.cacheTime": {
                    "type": "integer",
                    "default": 21600,
                    "description": "The length of time (in seconds) to cache library and search results"
                }
            }
        }
    },
    "devDependencies": {
        "vscode": "^1.1.33"
    },
    "dependencies": {
        "got": "^9.6.0",
        "open": "^6.2.0",
        "vscode-cache": "^0.3.0"
    }
}
